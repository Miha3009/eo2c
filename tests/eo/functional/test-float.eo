+package test
+alias org.eolang.seq

[] > test-float
  seq > @
    test-float-1
    test-float-2
    test-float-3
    test-float-4
    test-float-5
    test-float-6
    test-float-7
    test-float-8
    test-float-9
    test-float-10
    test-float-11

  [] > test-float-1
    if. > @
      eq.
        40-09-1E-B8-51-EB-85-1F-
        3.14.as-bytes
      QQ.io.stdout "OK TEST FLOAT 1\n"
      QQ.io.stdout "FAIL TEST FLOAT 1\n"

  [] > test-float-2
    if. > @
      eq.
        3.14
        3.14
      QQ.io.stdout "OK TEST FLOAT 2\n"
      QQ.io.stdout "FAIL TEST FLOAT 2\n"

  [] > test-float-3
    if. > @
      eq.
        4.0
        1.5.plus 2.5
      QQ.io.stdout "OK TEST FLOAT 3\n"
      QQ.io.stdout "FAIL TEST FLOAT 3\n"

  [] > test-float-4
    if. > @
      eq.
        2.25
        1.5.times 1.5
      QQ.io.stdout "OK TEST FLOAT 4\n"
      QQ.io.stdout "FAIL TEST FLOAT 4\n"

  [] > test-float-5
    if. > @
      eq.
        -1.5
        1.5.neg
      QQ.io.stdout "OK TEST FLOAT 5\n"
      QQ.io.stdout "FAIL TEST FLOAT 5\n"

  [] > test-float-6
    if. > @
      eq.
        0.75
        1.5.div 2.0
      QQ.io.stdout "OK TEST FLOAT 6\n"
      QQ.io.stdout "FAIL TEST FLOAT 6\n"

  [] > test-float-7
    if. > @
      gt.
        2.5
        1.5
      QQ.io.stdout "OK TEST FLOAT 7\n"
      QQ.io.stdout "FAIL TEST FLOAT 7\n"

  [] > test-float-8
    if. > @
      gte.
        1.5
        1.5
      QQ.io.stdout "OK TEST FLOAT 8\n"
      QQ.io.stdout "FAIL TEST FLOAT 8\n"

  [] > test-float-9
    if. > @
      lt.
        1.5
        2.5
      QQ.io.stdout "OK TEST FLOAT 9\n"
      QQ.io.stdout "FAIL TEST FLOAT 9\n"

  [] > test-float-10
    if. > @
      lte.
        1.5
        1.5
      QQ.io.stdout "OK TEST FLOAT 10\n"
      QQ.io.stdout "FAIL TEST FLOAT 10\n"

  [] > test-float-11
    if. > @
      eq.
        2.5
        4.0.minus 1.0 0.5
      QQ.io.stdout "OK TEST FLOAT 11\n"
      QQ.io.stdout "FAIL TEST FLOAT 11\n"
